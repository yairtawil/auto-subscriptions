{
  "name": "auto-subscriptions",
  "version": "2.0.1",
  "description": "A typescript class and propery decorators to manager all subscriptions",
  "main": "dist/public_api.js",
  "typings": "dist/public_api.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "predeploy": "npm run build:dev && npm run build --prefix example -- --prod --base-href=https://yairtawil.github.io/auto-subscriptions/ --deploy-url=https://yairtawil.github.io/auto-subscriptions/",
    "deploy": "gh-pages -d example/dist/example",
    "start:example": "npm run start --prefix example",
    "build:dev": "tsc -p tsconfig.dev.json -listEmittedFiles",
    "build:dev:watch": "npm run build:dev -- -w",
    "prepublish": "npm run build",
    "test": "jasmine-ts src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yairtawil/auto-subscriptions"
  },
  "keywords": [],
  "author": "Yair Tawil",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "gh-pages": "^1.2.0",
    "jasmine-ts": "^0.2.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.3.3333"
  }
}
