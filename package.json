{
  "name": "auto-subscriptions",
  "version": "2.0.5",
  "description": "A typescript class and propery decorators to manager all subscriptions",
  "main": "dist/public_api.js",
  "typings": "dist/public_api.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "predeploy": "npm run build && npm run build --prefix example -- --prod --base-href=https://yairtawil.github.io/auto-subscriptions/ --deploy-url=https://yairtawil.github.io/auto-subscriptions/",
    "deploy": "gh-pages -r https://$GITHUB_TOKEN@github.com/yairtawil/auto-subscriptions.git -d example/dist/example",
    "start:example": "npm run start --prefix example",
    "build:watch": "npm run build -- -w",
    "prepublish": "npm run build",
    "test": "jasmine-ts src/**/*.spec.ts",
    "lint": "tslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yairtawil/auto-subscriptions"
  },
  "keywords": [],
  "author": "Yair Tawil",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "gh-pages": "^2.0.1",
    "jasmine-ts": "^0.2.1",
    "rimraf": "^2.6.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  }
}
